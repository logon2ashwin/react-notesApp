{
    "env": {
      "browser": true,
      "node": true
    },
    "globals": {
      "mode": true
    },
    "plugins": [
      "react"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      // "no-console": [
      //   "warn"
      // ],
      "indent": [
        "error",
        2,
        {
          "SwitchCase": 1,
          "VariableDeclarator": 2
        }
      ],
      "no-trailing-spaces": [
        "error"
      ],
      "no-extra-semi": [
        "error"
      ],
      "no-whitespace-before-property": [
        "error"
      ],
      "no-unneeded-ternary": [
        "warn"
      ],
      "no-irregular-whitespace": [
        "error"
      ],
      "linebreak-style": 0,
      "quotes": [
        "error",
        "double"
      ],
      "semi": [
        "error",
        "always"
      ],
      "camelcase": [
        "error",
        {
          "properties": "always"
        }
      ],
      "eol-last": [
        "error",
        "always"
      ],
      "func-call-spacing": [
        "error",
        "never"
      ]
    }
  }